{
  "name": "build-test",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.3",
    "typescript": "^5.3.2"
  },
  "scripts": {
    "dev:fe": "parcel ./client/index.html",
    "dev:be": "nodemon --watch ./server/index.ts --exec ts-node ./server/index.ts",
    "build:fe": "parcel build ./client/index.html",
    "build:be": "tsc",
    "build": "npm run build:fe && npm run build:be",
    "start": "node ./dist/index.js"
  }
}
